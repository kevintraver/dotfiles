# https://github.com/tridactyl/tridactyl/blob/master/src/lib/config.ts

sanitise tridactyllocal tridactylsync

set modeindicator false

bind a js javascript:void(open('http://archive.today/?run=1&url='+encodeURIComponent(document.location)))
command archive js javascript:void(open('http://archive.today/?run=1&url='+encodeURIComponent(document.location)))

unbind d
unbind D

unbind g?

bind t fillcmdline tabopen
bind x tabclose
bind X tabonly

unbindurl x.com j
unbindurl x.com k

