[
  {
    "key": "cmd+o",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+cmd+e",
    "command": "workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "cmd+p",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "shift+cmd+o",
    "command": "projectManager.listProjects"
  },
  {
    "key": "cmd+d",
    "command": "workbench.action.splitEditor"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.navigateLeft",
    "when": "neovim.mode != insert"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.navigateRight",
    "when": "neovim.mode != insert"
  },
  {
    "key": "alt+h",
    "command": "vscode-neovim.send",
    "args": "<A-h>",
    "when": "editorTextFocus && neovim.mode != insert"
  },
  {
    "key": "alt+j",
    "command": "vscode-neovim.send",
    "args": "<A-j>",
    "when": "editorTextFocus && neovim.mode != insert"
  },
  {
    "key": "alt+k",
    "command": "vscode-neovim.send",
    "args": "<A-k>",
    "when": "editorTextFocus && neovim.mode != insert"
  },
  {
    "key": "alt+l",
    "command": "vscode-neovim.send",
    "args": "<A-l>",
    "when": "editorTextFocus && neovim.mode != insert"
  },
  {
    "key": "ctrl+p",
    "command": "-cursorUp",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+n",
    "command": "-cursorDown",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+p",
    "command": "vscode-neovim.send",
    "args": "<C-p>",
    "when": "editorTextFocus && neovim.mode != insert"
  },
  {
    "key": "ctrl+n",
    "command": "vscode-neovim.send",
    "args": "<C-n>",
    "when": "editorTextFocus && neovim.mode != insert"
  },
  {
    "key": "alt+d",
    "command": "git.openChange",
    "when": "editorTextFocus && neovim.mode != 'insert'"
  },
  {
    "key": "shift+alt+d",
    "command": "git.openAllChanges"
  },
  {
    "key": "alt+c",
    "command": "git.commit",
    "when": "neovim.mode != 'insert'"
  },
  {
    "key": "c",
    "command": "git.commit",
    "when": "focusedView == 'workbench.scm'"
  },
  {
    "key": "s",
    "command": "git.stage",
    "when": "focusedView == 'workbench.scm'"
  },
  {
    "key": "u",
    "command": "git.unstage",
    "when": "focusedView == 'workbench.scm'"
  },
  {
    "key": "r",
    "command": "git.clean",
    "when": "focusedView == 'workbench.scm'"
  },
  {
    "key": "alt+s",
    "command": "git.stageSelectedRanges",
    "when": "editorTextFocus && neovim.mode != 'insert'"
  },
  {
    "key": "shift+alt+s",
    "command": "git.stage",
    "when": "editorTextFocus && neovim.mode != 'insert'"
  },
  {
    "key": "alt+u",
    "command": "git.unstage",
    "when": "editorTextFocus && neovim.mode != 'insert'"
  },
  {
    "key": "alt+r",
    "command": "git.revertSelectedRanges",
    "when": "editorTextFocus && neovim.mode != 'insert'"
  },
  {
    "key": "alt+[",
    "command": "workbench.action.editor.nextChange",
    "when": "editorTextFocus && !textCompareEditorActive"
  },
  {
    "key": "alt+]",
    "command": "workbench.action.editor.previousChange",
    "when": "editorTextFocus && !textCompareEditorActive"
  },
  {
    "key": "cmd+enter",
    "command": "rest-client.request",
    "when": "editorTextFocus && (editorLangId == 'http' || editorLangId == 'graphql')"
  },
  {
    "key": "alt+.",
    "command": "vscode-neovim.send",
    "args": "<A-.>",
    "when": "editorTextFocus && neovim.mode != insert"
  },
  {
    "key": "alt+,",
    "command": "vscode-neovim.send",
    "args": "<A-,>",
    "when": "editorTextFocus && neovim.mode != insert"
  },
  {
    "key": "ctrl+space",
    "command": "vscode-neovim.send",
    "args": "<C-Space>",
    "when": "editorTextFocus && neovim.mode != 'insert'"
  },
  {
    "key": "shift+space",
    "command": "vscode-neovim.send",
    "args": "<S-Space>",
    "when": "editorTextFocus && neovim.mode != 'insert'"
  },
  {
    "key": "cmd-]",
    "command": "editor.action.marker.next",
    "when": "editorFocus"
  },
  {
    "key": "cmd-[",
    "command": "editor.action.marker.prev",
    "when": "editorFocus"
  },
  {
    "key": "cmd+h",
    "command": "editor.action.showHover",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+m",
    "command": "bookmarks.toggle",
    "when": "editorTextFocus && neovim.mode != 'insert'"
  },
  {
    "key": "ctrl+]",
    "command": "bookmarks.jumpToNext",
    "when": "editorTextFocus && neovim.mode != 'insert'"
  },
  {
    "key": "ctrl+[",
    "command": "bookmarks.jumpToPrevious",
    "when": "editorTextFocus && neovim.mode != 'insert'"
  },
  {
    "key": "ctrl+cmd+g",
    "command": "workbench.scm.focus"
  },
  {
    "key": "alt+g",
    "command": "magit.status"
  },
  {
    "key": "g g",
    "command": "cursorTop",
    "when": "editorTextFocus && editorLangId == 'magit' && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
  },
  {
    "key": "x",
    "command": "magit.discard-at-point",
    "when": "editorTextFocus && editorLangId == 'magit' && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
  },
  {
    "key": "k",
    "command": "-magit.discard-at-point"
  },
  {
    "key": "o",
    "command": "magit.visit-at-point",
    "when": "editorTextFocus && editorLangId == 'magit' && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
  },
  {
    "key": "ctrl+j",
    "command": "magit.move-next-entity",
    "when": "editorTextFocus && editorLangId == 'magit' && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
  },
  {
    "key": "ctrl+n",
    "command": "magit.move-next-entity",
    "when": "editorTextFocus && editorLangId == 'magit' && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
  },
  {
    "key": "ctrl+k",
    "command": "magit.move-previous-entity",
    "when": "editorTextFocus && editorLangId == 'magit' && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
  },
  {
    "key": "ctrl+p",
    "command": "magit.move-previous-entity",
    "when": "editorTextFocus && editorLangId == 'magit' && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
  }
]
