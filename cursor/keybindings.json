  {
    "key": "cmd+e",
    "command": "workbench.view.explorer",
    "when": "!explorerViewletVisible"
  },
  {
    "key": "cmd+m",
    "command": "composer.openModelToggle",
    "when": "composerFocused && !editorTextFocus"
  }
