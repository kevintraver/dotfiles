{
  "editor.minimap.enabled": false,
  "windsurf.autocompleteSpeed": "default",
  "windsurf.autoExecutionPolicy": "turbo",
  "windsurf.chatFontSize": "default",
  "windsurf.rememberLastModelSelection": true,
  "windsurf.openRecentConversation": true,
  "windsurf.explainAndFixInCurrentConversation": true,
  "windsurf.cascadeCommandsAllowList": [],
  "windsurf.cascadeOpenOnReload": false,
  "editor.formatOnSave": true,
  "git.autofetch": true,
  "scm.defaultViewMode": "tree",
  "files.dialog.defaultPath": "/Users/kevin/code",
  "gitlens.ai.model": "anthropic:claude-3-5-sonnet-latest",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.packageManager": "pnpm",
  "eslint.workingDirectories": ["apps", "packages"],
  "prettier.printWidth": 100,
  "prettier.arrowParens": "avoid",
  "prettier.enable": true,
  "prettier.endOfLine": "lf",
  "prettier.semi": false,
  "prettier.jsxSingleQuote": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "eslint.format.enable": true,

  "window.newWindowDimensions": "maximized",
  "extensions.ignoreRecommendations": true,
  "files.exclude": {
    "**/.next": true,
    "**/pnpm-lock.yaml": true
  },
  "search.useGlobalIgnoreFiles": true,
  "accessibility.signals.terminalBell": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.terminalCommandSucceeded": {
    "sound": "off",
    "announcement": "off"
  },
  "terminal.integrated.enableVisualBell": false,
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua"
  },
  "gitlens.plusFeatures.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "prettier.configPath": "~/.dotfiles/prettier/prettierrc",
  "editor.accessibilitySupport": "off",
  "security.promptForLocalFileProtocolHandling": false,
  "vscode-just.runInTerminal": true,
  "xstate.nestTypegenFiles": false,
  "amp.url": "https://ampcode.com/",
  "amp.tab.enabled": true,
  "editor.wordBasedSuggestions": "off",
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "amp.gpt5": true,
  "cursor.agent_layout_browser_beta_setting": true,
  "cursor.enable_agent_window_setting": true,
  "workbench.colorTheme": "Tokyo Night"
}
